public class Main {

//	Declare a private,default,protected,public methods and have them each return the name of the what access modifier 
//	they are using in order as below. make the return values add to each other.
//
//			Output:
//			private, default, protected, public
//	
	
	private String str() {
		String str = "private";
		System.out.println(str);
		return str;
	}
	String name() {
		String name = "default";
		System.out.println(name);
		return name;
	} 
	
	protected String prot() {
		String str1 = "protected";
		System.out.println(str1);
		return str1;
	}
	
	
	public String pub = "public";
	
	

	public static void main(String[] args) {
		Main out = new Main();
		out.str();
		out.name();
		out.prot();
	}
	
}	
	
	
	
	
	
	
	


	
	
	
	
	
	
	
	
	
	
	

		